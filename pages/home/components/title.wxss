/* pages/home/components/title.wxss */
.analysis-card{
  margin-top: 30px;
  padding: 26px 16px;
  width: -webkit-calc(100% - 32px);
  width: calc(100% - 32px);
  height: auto;
  border-radius: 20px;
  background-color: #ffffff;
}
.analysis-title{
  margin-top: 40px;
  width: 100%;
  text-align: center;
  font-size: 0;
  margin-bottom: 45px;
}
.title-sporadic{
  width: 9px;
  height: 9px;
  transform: rotate(45deg);
  background-color: #32CD32;
  display: inline-block;  
  margin-left: 10px;
}
.title-text{
  margin: 0 11px;
  display: inline-block; 
  color: #333333;
  font-size: 18px;
}